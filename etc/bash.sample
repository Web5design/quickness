### Short Cuts
alias dj="./manage.py runserver 0.0.0.0:8000"
alias findcss='find . -name "*.css" | xargs grep'
alias findhtml='find . -name "*.html" | xargs grep'
alias findjs='find . -name "*.js" | xargs grep'
alias findpy='find . -name "*.py" | xargs grep'
alias greppy='egrep "^ *(#|class |def )"'
alias rmpyc='find . -name "*.pyc" | xargs rm'


