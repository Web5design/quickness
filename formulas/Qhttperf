###
### Settings
###

APP_VERSION="httperf-0.9.0"
HTTPPERF_PREV_DIR=$PWD
cd $QUICKNESS_SRC


###
### Httperf
###

if [ -d $APP_VERSION ]; then
    rm -rf ./$APP_VERSION
fi

wget ftp://ftp.hpl.hp.com/pub/httperf/$APP_VERSION
tar xvzf $APP_VERSION.tar.gz
cd $APP_VERSION
./configure && make
sudo make install

cd $HTTPPERF_PREV_DIR
